   "metadata": {},
   "source": [
    "# N줄로 덧셈"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "63e74924",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5\n",
      "15\n"
     ]
    }
   ],
   "source": [
    "a = 0\n",
    "N = int(input())\n",
    "\n",
    "for i in range(1, N+1):\n",
    "    a += i\n",
    "print(a)"
   ]
